{
    "name": "applikacjonizm/crud-generator",
    "description": "This bundle generates a nice CRUD with Bootstrap 3 interface. Based on jordillonch/CrudGeneratorBundle",
    "keywords": ["crud", "generator"],
    "homepage": "https://github.com/applikacjonizm/CrudGeneratorBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Applikacjonizm",
            "email": "kontakt@applikacjonizm,pl"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.6.0, <2.7-dev",
        "sensio/generator-bundle": ">2.2.0, <2.4-dev",
        "pagerfanta/pagerfanta": "1.0.*@dev",
        "lexik/form-filter-bundle": "~3.0",
        "doctrine/orm": ">=2.2.3,<2.5-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Applikacjonizm\\Bundle\\CrudGeneratorBundle": "" }
    },
    "target-dir": "Applikacjonizm/Bundle/CrudGeneratorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.6.x-dev"
        }
    }
}
